<!-- prettier-ignore -->
<% if(success && success.length) {%>
<div class="row justify-content-center m-2">
  <div
    class="alert alert-success alert-dismissible fade show col-9"
    role="alert"
  >
    <%= success %>
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
</div>
<% } %> <% if(error && error.length) {%>
<div class="row justify-content-center m-2">
  <div
    class="alert alert-danger alert-dismissible fade show col-9"
    role="alert"
  >
    <%= error %>
    <button
      type="button"
      class="btn-close"
      data-bs-dismiss="alert"
      aria-label="Close"
    ></button>
  </div>
</div>
<% } %>
